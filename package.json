{
  "name": "@prospector-btc/source",
  "version": "10.6.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.0.0"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "tools/*"
  ],
  "scripts": {
    "postinstall": "husky",
    "i18n:generate": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"esModuleInterop\":true}' ts-node --project tsconfig.base.json apps/web-dashboard/tools/scripts/generate-i18n.ts",
    "build:web:production": "pnpm i18n:generate && nx build web-dashboard --prod",
    "audit:logic:linter": "nx run-many --target=lint --all --parallel=3",
    "audit:types:check": "nx run-many --target=type-check --all --parallel=3",
    "validate:system": "ts-node --project tools/scripts/tsconfig.json tools/scripts/validate-system.ts"},
  "devDependencies": {
    "@monodon/rust": "^2.3.0",
    "@nx/devkit": "^22.3.3",
    "@nx/eslint": "^22.1.3",
    "@nx/eslint-plugin": "22.1.3",
    "@nx/jest": "^22.1.3",
    "@nx/js": "22.1.3",
    "@nx/next": "^22.1.3",
    "@nx/react": "^22.1.3",
    "@nx/workspace": "22.1.3",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.0",
    "husky": "^9.1.7",
    "nx": "22.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "fingerprint-generator": "^2.1.78",
    "fingerprint-injector": "^2.1.78",
    "framer-motion": "^12.23.26",
    "ghost-cursor-playwright": "^2.1.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.0",
    "next-auth": "^4.24.13",
    "next-intl": "^4.6.1",
    "next-themes": "^0.4.6",
    "p-limit": "4.0.0",
    "playwright": "^1.40.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
