{
  "name": "@prospector-btc/source",
  "version": "8.5.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "tools/*"
  ],
  "scripts": {
    "initialize:environment": "pnpm install && pnpm i18n:generate",
    "dev:orchestrator:api": "nx serve orchestrator",
    "dev:dashboard:web": "nx serve web-dashboard",
    "dev:fullstack": "nx run-many --target=serve --projects=orchestrator,web-dashboard --parallel",
    "build:api:production": "nx build orchestrator --prod",
    "build:web:production": "pnpm i18n:generate && nx build web-dashboard --prod",
    "build:worker:static": "powershell ./scripts/build_miner_static.ps1",
    "build:all:strata": "nx run-many --target=build --all --prod",
    "audit:types": "nx run-many --target=type-check --all",
    "audit:logic:linter": "nx run-many --target=lint --all",
    "audit:format:fix": "nx format:write && cargo fmt",
    "test:suite:full": "nx run-many --target=test --all",
    "test:stratum:core": "nx run-many --target=test --projects=tag:layer:core",
    "test:stratum:infra": "nx run-many --target=test --projects=tag:layer:infra",
    "lab:generate:proof": "cargo run --bin prover -- --output dist/proof",
    "lab:verify:system": "ts-node --project tools/scripts/tsconfig.json tools/scripts/validate-system.ts",
    "ops:deploy:preflight": "ts-node --project tools/scripts/tsconfig.json tools/scripts/production-preflight.ts",
    "i18n:generate": "cross-env TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\",\\\"esModuleInterop\\\":true}\" ts-node --project tsconfig.base.json apps/web-dashboard/tools/scripts/generate-i18n.ts",
    "docker:build:orchestrator": "docker build -f apps/orchestrator/Dockerfile -t prospector/orchestrator .",
    "docker:build:dashboard": "docker build -f apps/web-dashboard/Dockerfile -t prospector/web-dashboard ."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/supabase-js": "^2.87.3",
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.2",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "fingerprint-generator": "^2.1.78",
    "fingerprint-injector": "^2.1.78",
    "framer-motion": "^11.15.0",
    "ghost-cursor-playwright": "^2.1.0",
    "lucide-react": "^0.469.0",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "^3.26.3",
    "next-themes": "^0.4.4",
    "playwright": "^1.57.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "zod": "^3.24.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@monodon/rust": "^1.3.0",
    "@nx/devkit": "20.2.1",
    "@nx/eslint": "20.2.1",
    "@nx/jest": "20.2.1",
    "@nx/js": "20.2.1",
    "@nx/next": "20.2.1",
    "@nx/react": "20.2.1",
    "@nx/workspace": "20.2.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/eventsource": "^1.1.15",
    "@types/node": "22.10.5",
    "@types/react": "19.0.4",
    "@types/react-dom": "19.0.2",
    "@types/uuid": "^10.0.0",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "10.4.20",
    "cross-env": "^10.1.3",
    "eslint": "9.18.0",
    "eslint-config-next": "16.1.0",
    "eventsource": "^4.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "next": "15.4.10",
    "nx": "22.2.1",
    "postcss": "8.5.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "uuid": "^12.0.0",
    "webpack": "^5.104.1"
  }
}
