  "scripts": {
    "--- ORCHESTRATION ---": "--------------------------------",
    "start": "pnpm i18n:gen && nx run-many --target=serve --projects=orchestrator,web-dashboard --parallel",
    "start:clean": "pnpm clean && pnpm start",
    "--- BUILDING ---": "-------------------------------------",
    "build:all": "pnpm i18n:gen && nx run-many --target=build --all --prod",
    "build:web": "pnpm i18n:gen && nx build web-dashboard --prod",
    "build:api": "nx build orchestrator --prod",
    "--- DOCKER OPS ---": "-----------------------------------",
    "docker:build:web": "docker build -f apps/web-dashboard/Dockerfile -t prospector/web-dashboard .",
    "docker:build:api": "docker build -f apps/orchestrator/Dockerfile -t prospector/orchestrator .",
    "docker:up": "docker-compose up -d",
    "--- UTILS ---": "----------------------------------------",
    "i18n:gen": "ts-node tools/scripts/generate-i18n.ts",
    "clean": "rimraf dist target .nx apps/web-dashboard/messages",
    "lint:fix": "nx run-many --target=lint --all --fix"
  },
